[
  {
    "context": "VimControl && !menu",
    "bindings": {
      "g D": "editor::GoToTypeDefinition",
      "g -": "project_panel::ToggleFocus",
      "g r": "editor::Rename",
      "g y": "workspace::CopyPath",
      "g o": "editor::OrganizeImports",
      "space n": "editor::GoToDiagnostic",
      "space N": "editor::GoToPreviousDiagnostic",
      "space c": "editor::GoToHunk",
      "space C": "editor::GoToPreviousHunk",
      "cmd-o": "pane::GoBack",
      "cmd-i": "pane::GoForward",
      "cmd-d": "vim::ScrollDown",
      "cmd-u": "vim::ScrollUp",
      "cmd-r": "editor::Redo",
      "cmd-e": "vim::LineDown",
      "cmd-h": "workspace::ActivatePreviousPane",
      "cmd-l": "workspace::ActivateNextPane",
      "cmd-y": "vim::LineUp",
      ", w": "workspace::Save",
      ", W": "workspace::SaveAll",
      ", m": "editor::FoldAll",
      ", r": "editor::UnfoldAll",
      ", t": "editor::ToggleSelectedDiffHunks",
      ", d": "git::Diff",
      ", z": ["workspace::SendKeystrokes", "alt-cmd-z"],
      ", -": "editor::OpenExcerpts",
      ", v": "pane::SplitVertical",
      ", b": "editor::ToggleGitBlame",
      "cmd-B": "workspace::ToggleRightDock",
      "g t": "vim::GoToTab",
      "g T": "vim::GoToPreviousTab",
      "g m": "vim::ToggleMarksView"
    }
  },
  {
    "context": "Editor && vim_mode == normal",
    "bindings": {
      ", h": ["workspace::SendKeystrokes", "^"],
      ", l": ["workspace::SendKeystrokes", "$"]
    }
  },
  {
    "context": "Editor && vim_mode == insert",
    "bindings": {
      ", c": ["workspace::SendKeystrokes", "escape"]
    }
  },
  {
    "context": "EmptyPane",
    "bindings": {
      "cmd-o": "workspace::Open",
      "g -": "project_panel::ToggleFocus"
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "cmd-t": "workspace::NewTerminal"
    }
  }
]
